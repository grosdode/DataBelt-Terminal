{
  "name": "data_belt",
  "version": "1.0.1",
  "description": "Application to communicate with a DataBelt",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "publish": "electron-builder -p always"
  },
  "build": {
    "appId": "data_belt_terminal",
    "productName": "DataBelt-Terminal",
    "directories": {
      "output": "electron-build"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico",
      "publish": [
        "github"
      ]
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "icon.ico",
      "uninstallDisplayName": "DataBlet-Uninstaller",
      "license": "LICENSE",
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": "Dominik G.",
  "license": "MIT",
  "devDependencies": {
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0"
  }
}
